package fizzBuzz;

public class fizzBuzz {
	public static int n = 20;
	public static void main(String[] args) {
		for(int i=1;i<=n;i++) {
			if(i%3==0) {
				System.out.printf("Fizz");
			}
			if(i%5==0) {
				System.out.printf("Buzz");
			}
			if(i%3==0 && i%5==0) {
				System.out.printf("FizzBuzz");
			}
			System.out.printf(i);
	}
  }
}
