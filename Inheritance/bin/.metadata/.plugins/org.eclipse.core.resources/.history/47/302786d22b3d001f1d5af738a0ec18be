package chuong3;

public class bai90_update {
	public static int n;
	public static int findingm(int n) {
		//1+2+3+4+5+6<20
		//expected : return 5 
		int i=0;
		int sum=0;
		while(sum<n) {
			for(i=1;i<n;i++) {
				sum+=i;
			}
		}
		return i-1;
	}
	public static void main(String[] args) {
		System.out.println(findingm(20)); 
	}

}
