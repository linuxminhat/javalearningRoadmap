package overriding;

public class overridinglearnign {
	public class Vehicle() {
		public void run() {
			System.out.println("Vehicle is  Running");
		}
	}
	public class Bike extends Vehicle{
		 public void run() {
			System.out.println("Bike is running");
		}
	}
	public static void main(String[] args) {
		Bike bike1 = new Bike();
		bike1.run();
	}
	

}
