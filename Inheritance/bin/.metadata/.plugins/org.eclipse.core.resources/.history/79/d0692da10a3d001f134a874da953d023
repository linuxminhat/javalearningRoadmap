public class bai53 {
	public static int number;
	public static int findingNumbExistMax(int number) {
		int numbExistMax=1;
		int available_count=1;
		if(number<9) {
			return number;
		}
		while(number>0) {
			int digits=number%10;
			if(digits==numbExistMax) {
				count+=1;
				return digits;
			}
			if(digits>numbExistMax) {
				numbExistMax=digits;
				
			}
		}
		
	}
	public static void main(String[] args) {
		
	}

}
