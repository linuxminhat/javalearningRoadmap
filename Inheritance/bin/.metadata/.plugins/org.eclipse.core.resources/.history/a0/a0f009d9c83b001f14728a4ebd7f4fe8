package objectComposition_Exercisee;

public class book {
	public String id;
	public String name;
	public author author;
	//declare get and set 
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id=id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name=name;
	}
	
	
		
}