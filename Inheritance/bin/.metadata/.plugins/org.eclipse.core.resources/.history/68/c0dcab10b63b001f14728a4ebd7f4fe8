package objectComposition_updateExercise;

public class square {
	public int length;
	public int width;
	public square(int length,int width) {
		this.length=length;
		this.width=width;
	}
	public int area() {
		return length*width;
	}
	public int perimeter() {
		return 2*(width+length);
	}
	public String toString() {
		return "length"+length+"width"+width+"area"+area+"perimeter"+perimeter;
	}

}
