package arrays_hashing;

public class leetcode88_review {
	public void merge(int[] nums1, int m, int[] nums2, int n) {
		for(int i:nums2) {
			chen(i,nums1,m);
			m++;
		}
		 
    }

	private void chen(int i, int[] nums1, int m) {
		/*
		 * 1 2 3 0 0 0 / 2 5 6 
		 * 2<3 nen chen vao vi tri cua 3 
		 */
		for(int k=0;k<nums1.length;i++) {
			if(nums1[k]>i) {
				nums1[k]=i;
			}
		}
		
	}
	 

}
