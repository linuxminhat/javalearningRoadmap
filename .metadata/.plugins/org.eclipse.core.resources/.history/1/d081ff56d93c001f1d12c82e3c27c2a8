package sortElementsOfArray;

public class sorting {
	static int[] array = { 100,2,424,13,53,122,0,4};
	//want to sort : shortest -> biggest 
	public int[] sortingArray(int[] array) {
		int minValue = array[0];
		int temp_value;
		for(int i=1;i<array.length;) {
			if(array[i]<array[0]) {
				temp_value=array[0];
				array[i]=temp_value;
				array[0]=array[i];
				i++;
				do {
					
				}while(i<array.length+1);
			}
		}
	return null; 
	}

}
