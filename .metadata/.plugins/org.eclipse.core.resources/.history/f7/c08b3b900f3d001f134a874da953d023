	public static boolean checkingGreater(int number) {
		if(number<=9) {
			return true;
		}
		if(number<=99) {
			while(number>0) {
				//98
				int lastdigit=number%10;//8
				int firstdigit=number/10;//9
				
				
			}
		}
	}
	public static void main(String[] args) {
		
	}

