public class stringJoinFunction {
	public static void main(String[] args) {
		String str = String.join("-", "Hello","String","Join");
		//Ki tu dau tien la ki tu ma minh muon noi 
		//Cac ki tu con lai se tao thanh chuoi khi noi xong 
		String str1 = String.join("-", "hello","minh");
		 
	}

}
