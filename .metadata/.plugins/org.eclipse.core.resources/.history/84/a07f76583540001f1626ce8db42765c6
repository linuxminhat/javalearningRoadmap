package sorting;

public class insertionSort_review2 {
	//declare array
	public static int[] array = {5,3,2,7,8,1,2 };
	//implementing insertion sort function
	public static void insertionSort(int[] array) {
		for(int i=0;i<array.length;i++) {
			int ai=array[i];
			int j=i-1;
			while(j>0 && a[j]>ai) {
				a[j+1]=a[j];
				j--;
			}
			a[j+1]=ai;
		}
	}

}
