package chuong1;

public class b29 {
	public static int number;
	public static int maxDivisors(int number) {
		int maxNumb = Integer.MAX_VALUE;
		for(int numb=1;numb<number;numb++) {
			if(number%numb==0) {
				if(numb>maxNumb) {
					maxNumb=numb;
				}
			}
//			return numb;
		}
		return numb;
	}
	public static void main(String[] args) {
		
	}

}
