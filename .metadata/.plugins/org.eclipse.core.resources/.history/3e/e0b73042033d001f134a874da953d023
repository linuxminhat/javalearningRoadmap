package chuong1;

public class b44 {
	public static int number;
	public static int coutingSumOfNumb(int number) {
		int sum=0;
		int digit=number%10;
		while(number>0) {
			number/=10;
			int digit=number%10;
			sum+=digit;
			number/=10;
		}
		return digit;
	}
	public static void main(String[] args) {
		
	}
}
